{
  "name": "tov-figma-plugin-server",
  "version": "1.0.0",
  "description": "Ton of Voice Figma plugin server",
  "main": "app.ts",
  "author": "",
  "keywords": [],
  "license": "ISC",
  "type": "module",
  "scripts": {
    "start": "node app.ts",
    "dev": "node --watch app.ts",
    "develop": "concurrently \"node --watch app.ts\" \"npm run watch:js\"",
    "build:js": "esbuild client/main.js --bundle --minify --outfile=public/main.js",
    "watch:js": "esbuild client/main.js --bundle --minify --outfile=public/main.js --watch"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "better-auth": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "openai": "^5.12.0",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.8",
    "prisma": "^6.12.0"
  }
}
